{
  "name": "eslint-plugin-one",
  "version": "0.0.0",
  "description": "all in one",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "MIT",
  "author": "juck",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./mdParser": {
      "import": "./lib/parser/mdParser.js",
      "require": "./lib/parser/mdParser.js",
      "node": "./lib/parser/mdParser.js",
      "default": "./lib/parser/mdParser.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "dist",
    "lib/**/*.js"
  ],
  "scripts": {
    "clean": "rimraf node_modules -rf",
    "docs": "eslint-doc-generator",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "prepublishOnly": "npm unpublish eslint-plugin-one@0.0.0 --force",
    "prepublish": "npm unpublish eslint-plugin-one@0.0.0 --force",
    "t": "mocha tests/lib/rules/prefer-comment-method.js",
    "test": "mocha tests --recursive"
  },
  "dependencies": {
    "markdownlint": "^0.31.0",
    "marked": "^9.0.0",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.3",
    "@typescript-eslint/utils": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-doc-generator": "^1.4.3",
    "eslint-plugin-eslint-plugin": "^5.1.0",
    "eslint-plugin-markdownlint": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-yml": "^1.9.0",
    "mocha": "^10.0.0",
    "outdent": "^0.8.0"
  },
  "peerDependencies": {
    "eslint": ">=7"
  },
  "engines": {
    "node": "^14.17.0 || ^16.0.0 || >= 18.0.0"
  }
}
